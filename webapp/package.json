{
    "name": "agile-board.webapp",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "CI=true react-scripts test",
        "test:watch": "react-scripts test",
        "eject": "react-scripts eject",
        "watch": "sass --watch src:src",
        "build-css": "sass --style=compressed --no-source-map src:src",
        "lint": "eslint --fix \"src/**/*.js\"",
        "format": "prettier --write \"src/**/{*.js,*.scss}\""
    },
    "dependencies": {
        "@blueprintjs/core": "^3.19.1",
        "@blueprintjs/select": "^3.11.1",
        "axios": "^0.19.0",
        "classnames": "^2.2.6",
        "history": "^4.10.1",
        "react": "^16.10.1",
        "react-dnd-cjs": "^9.4.0",
        "react-dnd-html5-backend-cjs": "^9.4.0",
        "react-dnd-multi-backend": "^4.0.0",
        "react-dnd-touch-backend-cjs": "^9.4.0",
        "react-dom": "^16.10.1",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^3.2.0",
        "sass": "^1.23.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/dom": "^6.8.1",
        "@testing-library/react": "^9.3.0",
        "@types/jest": "^24.0.21",
        "eslint": "^6.6.0",
        "eslint-plugin-react": "^7.16.0",
        "eslint-plugin-react-hooks": "^2.1.2",
        "prettier": "^1.18.2"
    }
}
